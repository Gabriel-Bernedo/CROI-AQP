            app.P_ModificarElectrodomesticos_1.Visible = "off";
        case "1.1" % Click ingresar recibo
            app.P_IngresarRecibo_1.Visible = "on";
            add.P_Botones_Recibo_1.Visible = "on";
            app.P_Inicio_1.Visible = "off";
