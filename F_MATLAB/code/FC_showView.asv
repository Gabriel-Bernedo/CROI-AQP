            app.P_TablaRecibo_Consumo_1.Visible = "off";
            app.P_IngresarRecibo_1.Visible = "off";
            app.P_Inicio_1.Visible = "on";
        case "1.1.3" % Click en a√±adir en recibo
            app.P_Inicio_1.Visible = "off";
            app.P_IngresarRecibo_1.Visible = "on";
